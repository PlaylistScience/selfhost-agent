{% extends 'base.html.twig' %}

{% block body %}
<h1>Imported Youtube</h1>
<ul>
    {% for link in youtube %}
    <li>
        <a href="{{ path('playYoutube', {'id': link.filename}) }}">{{ link.filename }}</a>
    </li>
    {% endfor %}
</ul>

<a href="{{ path('importYoutube') }}"><h2>Import New</h2></a>
<hr/>

<h1>Imported Soundcloud</h1>
<ul>
    {% for link in soundcloud %}
    <li>
        <a href="{{ path('playSoundcloud', {'id': link.filename}) }}">{{ link.filename }}</a>
    </li>
    {% endfor %}
</ul>

<a href="{{ path('importSoundcloud') }}"><h2>Import New</h2></a>
<hr/>

{% endblock %}